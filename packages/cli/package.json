{
  "name": "@agentskills/cli",
  "version": "0.1.0",
  "description": "Command-line interface for Agent Skills management",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "agentskills": "./dist/index.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "node esbuild.config.js",
    "dev": "tsc -p tsconfig.build.json --watch",
    "clean:build": "rimraf dist",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@agentskills/core": "workspace:*",
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "gray-matter": "4.0.3",
    "js-yaml": "^4.1.0",
    "ora": "^8.0.1",
    "pacote": "21.3.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.7",
    "esbuild": "0.27.3",
    "rimraf": "^6.0.1",
    "typescript": "^5.7.3",
    "vitest": "^3.0.3"
  },
  "keywords": [
    "agentskills",
    "cli",
    "skills-management"
  ],
  "author": "Oliver JÃ¤gle <github@beimir.net>",
  "license": "MIT"
}
