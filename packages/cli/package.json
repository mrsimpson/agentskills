{
  "name": "@codemcp/agentskills-cli",
  "version": "1.6.1",
  "description": "Command-line interface for Agent Skills management",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "agentskills": "./dist/index.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json && chmod +x dist/index.js",
    "dev": "tsc -p tsconfig.build.json --watch",
    "clean:build": "rimraf dist",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@codemcp/agentskills-core": "workspace:*",
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "inquirer": "13.2.5",
    "ora": "^8.0.1"
  },
  "devDependencies": {
    "@types/inquirer": "9.0.9",
    "@types/node": "^22.10.7",
    "@typescript-eslint/eslint-plugin": "^8.21.0",
    "@typescript-eslint/parser": "^8.21.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "prettier": "^3.4.2",
    "rimraf": "^6.0.1",
    "typescript": "^5.7.3",
    "typescript-eslint": "8.21.0",
    "vitest": "^3.0.3"
  },
  "keywords": [
    "agentskills",
    "cli",
    "skills-management"
  ],
  "author": "Oliver JÃ¤gle <github@beimir.net>",
  "license": "MIT"
}
