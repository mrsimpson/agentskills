{
  "name": "@agentskills/mcp-server",
  "version": "0.1.0",
  "description": "MCP server implementation for Agent Skills",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "agentskills-mcp": "./dist/bin.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "dev": "tsc -p tsconfig.build.json --watch",
    "clean:build": "rimraf dist",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@agentskills/core": "workspace:*",
    "@modelcontextprotocol/sdk": "^1.19.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.7",
    "rimraf": "^6.0.1",
    "typescript": "^5.7.3",
    "vitest": "^3.0.3"
  },
  "keywords": [
    "agentskills",
    "mcp",
    "server"
  ],
  "author": "Oliver JÃ¤gle <github@beimir.net>",
  "license": "MIT"
}
